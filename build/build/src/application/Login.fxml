<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="121.0" prefWidth="114.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <AnchorPane prefHeight="304.0" prefWidth="516.0">
         <children>
            <Label layoutX="229.0" layoutY="24.0" prefHeight="17.0" prefWidth="58.0" text="Login" />
            <Label layoutX="122.0" layoutY="85.0" text="Username/email:" />
            <Label layoutX="162.0" layoutY="144.0" text="Password:" />
            <TextField fx:id="usernameField" layoutX="277.0" layoutY="80.0" />
            <PasswordField fx:id="passwdField" layoutX="277.0" layoutY="139.0" />
            <Button fx:id="loginBtn" layoutX="299.0" layoutY="235.0" mnemonicParsing="false" onAction="#loginBtnPressed" text="Login" />
            <Button fx:id="registerBtn" layoutX="192.0" layoutY="235.0" mnemonicParsing="false" onAction="#registerBtnPressed" text="Register" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
