<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.DisplayQuizToStudentController">
   <children>
      <AnchorPane prefHeight="398.0" prefWidth="404.0">
         <children>
            <Label layoutX="14.0" layoutY="14.0" prefHeight="45.0" prefWidth="379.0" text="Select a Quiz ID (you should have received this via email)" />
            <ChoiceBox fx:id="quizidCB" layoutX="127.0" layoutY="172.0" prefWidth="150.0" />
            <Label layoutX="14.0" layoutY="59.0" text="The choice box will display all your pending quizzes">
               <font>
                  <Font size="9.0" />
               </font>
            </Label>
            <Button fx:id="enterBtn" layoutX="277.0" layoutY="302.0" mnemonicParsing="false" onAction="#enterBtnPressed" text="Enter" />
            <Button fx:id="backBtn" layoutX="75.0" layoutY="302.0" mnemonicParsing="false" onAction="#backBtnPressed" text="Back" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
