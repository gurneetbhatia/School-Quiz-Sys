<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ChangePasswordController">
   <children>
      <AnchorPane prefHeight="325.0" prefWidth="488.0">
         <children>
            <Label layoutX="110.0" layoutY="73.0" text="Current Password:" />
            <Label layoutX="129.0" layoutY="129.0" text="New Password:" />
            <Label layoutX="71.0" layoutY="180.0" text="Re-enter New Password:" />
            <PasswordField fx:id="currentPasswordField" layoutX="254.0" layoutY="68.0" />
            <PasswordField fx:id="passwd1Field" layoutX="254.0" layoutY="124.0" />
            <PasswordField fx:id="passwd2Field" layoutX="254.0" layoutY="175.0" />
            <Button fx:id="doneBtn" layoutX="302.0" layoutY="257.0" mnemonicParsing="false" onAction="#doneBtnPressed" text="Done" />
            <Button fx:id="cancelBtn" layoutX="147.0" layoutY="257.0" mnemonicParsing="false" onAction="#cancelBtnPressed" text="Cancel" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
